Object.entries(values);\n\n  const handleClick = () => {\n    handleSelectedPokemon && handleSelectedPokemon();\n  };\n\n  return (\n    <div className={classNamesPokemonCard} onClick={handleClick}>\n      <div className={s.cardFront}>\n        <div className={cn(s.wrap, s.front)}>\n          <div className={cn(s.pokemon, s[type], s[possession])}>\n            <div className={s.values}>\n              {valuesToArray.map(([key, value]) => {\n                return (\n                  <div className={cn(s.count, s[key])} key={key}>\n                    {value}\n                  </div>\n                );\n              })}\n            </div>\n            <div className={s.imgContainer}>\n              <img src={img} alt={name} />\n            </div>\n            {!minimize && (\n              <div className={s.info}>\n                <span className={s.number}>#{id}</span>>\n                <h3 className={s.name}>{name}</h3>\n                <small className={s[type]}>\n                  Type: <span>{type}</span>\n                </small>\n              </div>\n            )}\n          </div>\n        </div>\n      </div>\n\n      <div className={s.cardBack}>\n        <div className={cn(s.wrap, s.back)}>\n          <img src={imgBack} alt='Ð¡ard Backed' id={id} />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default PokemonCard;\n","/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Menu/index.js",[],["163","164"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/MenuHeader/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/service/firebase.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Header/index.js",[],["165","166"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Button/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/routes/Finish/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/routes/Start/index.js",["167"],"import { useState, useEffect } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { useSelector, useDispatch } from 'react-redux';\nimport PokemonCard from '../../../../components/PokemonCard';\nimport Button from '../../../../components/Button';\n\nimport s from './Start.module.css';\n\nimport { actions, asyncActions, selectors } from '../../../../slices';\n\nconst StartPage = () => {\n  const pokemons = useSelector(selectors.selectPokemonsData);\n  const selectedPokemons = useSelector(selectors.selectGameSelectedPokemons);\n\n  const dispatch = useDispatch();\n\n  const history = useHistory();\n\n  const [pokemonsList, setPokemons] = useState({});\n\n  const selectedPokemonsLength = Object.keys(selectedPokemons).length;\n\n  useEffect(() => {\n    dispatch(asyncActions.getPokemons());\n  }, []);\n\n  useEffect(() => {\n    setPokemons(pokemons);\n  }, [pokemons]);\n\n  const handleSelectedPokemon = (key) => {\n    const selectedPokemon = { ...pokemonsList[key] };\n    dispatch(actions.addSelectedPokemon({ key, selectedPokemon }));\n\n    setPokemons((prevState) => {\n      return {\n        ...prevState,\n        [key]: {\n          ...prevState[key],\n          selected: !prevState[key].selected,\n        },\n      };\n    });\n  };\n\n  const handleOpenBoard = () => {\n    history.push('/game/board');\n  };\n\n  return (\n    <div className={s.container}>\n      <div className={s.buttonWrap}>\n        <Button onClick={handleOpenBoard} disabled={selectedPokemonsLength < 5}>\n          Start Game\n        </Button>\n      </div>\n      <div className={s.flex}>\n        {Object.entries(pokemonsList).map(\n          ([key, { type, values, name, img, id, selected }]) => {\n            return (\n              <PokemonCard\n                key={key}\n                type={type}\n                values={values}\n                name={name}\n                img={img}\n                id={id}\n                handleSelectedPokemon={() => {\n                  if (selectedPokemonsLength < 5 || selected) {\n                    handleSelectedPokemon(key);\n                  }\n                }}\n                active={true}\n                selected={selected}\n                className={s.card}\n              />\n            );\n          }\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default StartPage;\n","/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/routes/Board/index.js",["168","169"],"import { useEffect, useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { useSelector, useDispatch } from 'react-redux';\n\nimport PokemonCard from '../../../../components/PokemonCard';\nimport PlayerBoard from './components/PlayerBoard/insex';\nimport Result from './components/Result';\n\nimport routes from '../../../../service/routes';\nimport s from './Board.module.css';\nimport ArrowChoice from '../../../../components/ArrowChoice';\nimport { counterWin, getFirstStepInGame } from '../../../../utils';\nimport { actions, asyncActions, selectors } from '../../../../slices';\n\nconst renderArrowChoise = (firstStep) =>\n  firstStep === 0 ? (\n    <ArrowChoice />\n  ) : (\n    <ArrowChoice stop={true} side={firstStep} />\n  );\n\nconst BoardPage = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const selectedPokemons = useSelector(selectors.selectGameSelectedPokemons);\n  const player2Pokemons = useSelector(selectors.selectGamePlayer2Pokemons);\n  const result = useSelector(selectors.selectGameResult);\n\n  const [board, setBoard] = useState([]);\n  const [player1, setPlayer1] = useState(() => {\n    return Object.values(selectedPokemons).map((pokemon) => {\n      return {\n        ...pokemon,\n        possession: 'blue',\n      };\n    });\n  });\n  const [player2, setPlayer2] = useState([]);\n\n  const [selectedCard, setSelectedCard] = useState(null);\n  const [step, setStep] = useState(0);\n  const [whoseStep, setWhoseStep] = useState(0);\n\n  if (Object.keys(selectedPokemons).length === 0) {\n    history.replace('/game');\n  }\n\n  useEffect(() => {\n    const getData = async () => {\n      const boardResponse = await fetch(routes.getBoard.url);\n      const boardRequest = await boardResponse.json();\n\n      setBoard(boardRequest.data);\n\n      dispatch(asyncActions.getPlayer2Pokemons());\n    };\n\n    getData();\n  }, []);\n\n  useEffect(() => {\n    setPlayer2(() => {\n      return player2Pokemons.map((pokemon) => ({\n        ...pokemon,\n        possession: 'red',\n      }));\n    });\n  }, [player2Pokemons]);\n\n  useEffect(() => {\n    if (step === 9) {\n      const [count1, count2] = counterWin(board, player1, player2);\n\n      if (count1 === count2) {\n        dispatch(actions.addResult('draw'));\n      } else {\n        count1 > count2\n          ? dispatch(actions.addResult('win'))\n          : dispatch(actions.addResult('lose'));\n      }\n\n      setTimeout(() => {\n        history.push('/game/finish');\n      }, 5000);\n    }\n  }, [step]);\n\n  useEffect(() => {\n    const playerNumber = getFirstStepInGame(1, 2);\n\n    setTimeout(() => {\n      setWhoseStep(playerNumber);\n    }, 3000);\n  }, []);\n\n  const filterPlayerPokemons = (prevState) =>\n    prevState.filter(({ id }) => id !== selectedCard.id);\n\n  const handleClickBoardPlate = async (position) => {\n    if (selectedCard) {\n      const params = {\n        position,\n        card: selectedCard,\n        board,\n      };\n\n      const res = await fetch(routes.postCard.url, {\n        method: routes.postCard.method,\n        headers: routes.postCard.headers,\n        body: JSON.stringify(params),\n      });\n\n      const request = await res.json();\n\n      selectedCard.player === 1\n        ? setPlayer1(filterPlayerPokemons)\n        : setPlayer2(filterPlayerPokemons);\n\n      setBoard(request.data);\n\n      setStep((prevState) => {\n        const count = prevState + 1;\n        return count;\n      });\n\n      setWhoseStep((prevState) => (prevState === 1 ? 2 : 1));\n    }\n  };\n\n  const handleClickCard = (card) => {\n    setSelectedCard(card);\n  };\n\n  return (\n    <div className={s.root}>\n      <div className={s.playerOne}>\n        <PlayerBoard\n          player={1}\n          cards={player1}\n          onClickCard={(card) => handleClickCard(card)}\n          whoseStep={whoseStep === 1}\n        />\n      </div>\n\n      <div className={s.board}>\n        {board.map(({ position, card }) => (\n          <div\n            className={s.boardPlate}\n            key={position}\n            onClick={() => !card && handleClickBoardPlate(position)}\n          >\n            {card && (\n              <PokemonCard {...card} minimize={true} className={s.card} />\n            )}\n          </div>\n        ))}\n      </div>\n\n      {!selectedCard && renderArrowChoise(whoseStep)}\n\n      {result && <Result type={result} />}\n\n      <div className={s.playerTwo}>\n        <PlayerBoard\n          player={2}\n          cards={player2}\n          onClickCard={(card) => handleClickCard(card)}\n          whoseStep={whoseStep === 2}\n        />\n      </div>\n    </div>\n  );\n};\n\nexport default BoardPage;\n","/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Navbar/index.js",[],["170","171"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/context/fireBaseContext.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/HomePage/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Layout/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/service/routes.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-ga