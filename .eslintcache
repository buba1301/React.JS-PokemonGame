s",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/gameRules.js",[],["166","167"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/PokemonCard/index.js",["168"],"import { useState } from 'react';\nimport cn from 'classnames';\nimport s from './PokemonCard.module.css';\n\nimport imgBack from './assets/cardBack.jpg';\n\nconst PokemonCard = ({\n  type,\n  values,\n  name,\n  img,\n  id,\n  handleSelectedPokemon,\n  active,\n  selected,\n  minimize = false,\n  possession,\n  className,\n}) => {\n  const classNamesPokemonCard = cn(className, s.pokemonCard, {\n    [s.active]: true,\n    [s.selected]: selected,\n  });\n\n  const valuesToArray = Object.entries(values);\n\n  const handleClick = () => {\n    handleSelectedPokemon && handleSelectedPokemon();\n  };\n\n  return (\n    <div className={classNamesPokemonCard} onClick={handleClick}>\n      <div className={s.cardFront}>\n        <div className={cn(s.wrap, s.front)}>\n          <div className={cn(s.pokemon, s[type], s[possession])}>\n            <div className={s.values}>\n              {valuesToArray.map(([key, value]) => {\n                return (\n                  <div className={cn(s.count, s[key])} key={key}>\n                    {value}\n                  </div>\n                );\n              })}\n            </div>\n            <div className={s.imgContainer}>\n              <img src={img} alt={name} />\n            </div>\n            {!minimize && (\n              <div className={s.info}>\n                <span className={s.number}>#{id}</span>>\n                <h3 className={s.name}>{name}</h3>\n                <small className={s[type]}>\n                  Type: <span>{type}</span>\n                </small>\n              </div>\n            )}\n          </div>\n        </div>\n      </div>\n\n      <div className={s.cardBack}>\n        <div className={cn(s.wrap, s.back)}>\n          <img src={imgBack} alt='Ð¡ard Backed' id={id} />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default PokemonCard;\n","/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Menu/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/MenuHeader/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/service/firebase.js",[],["169","170"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Header/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Button/index.js",[],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/routes/Finish/index.js",["171"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/routes/Start/index.js",["172","173","174","175"],"/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/routes/GamePage/routes/Board/index.js",["176","177"],"import { useEffect, useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { useSelector, useDispatch } from 'react-redux';\n\nimport PokemonCard from '../../../../components/PokemonCard';\nimport PlayerBoard from './components/PlayerBoard/insex';\nimport Result from './components/Result';\n\nimport routes from '../../../../service/routes';\nimport s from './Board.module.css';\nimport ArrowChoice from '../../../../components/ArrowChoice';\nimport { counterWin, getFirstStepInGame } from '../../../../utils';\nimport { actions, asyncActions, selectors } from '../../../../slices';\n\nconst renderArrowChoise = (firstStep) =>\n  firstStep === 0 ? (\n    <ArrowChoice />\n  ) : (\n    <ArrowChoice stop={true} side={firstStep} />\n  );\n\nconst BoardPage = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const selectedPokemons = useSelector(selectors.selectGameSelectedPokemons);\n  const player2Pokemons = useSelector(selectors.selectGamePlayer2Pokemons);\n  const result = useSelector(selectors.selectGameResult);\n\n  const [board, setBoard] = useState([]);\n  const [player1, setPlayer1] = useState(() => {\n    return Object.values(selectedPokemons).map((pokemon) => {\n      return {\n        ...pokemon,\n        possession: 'blue',\n      };\n    });\n  });\n  const [player2, setPlayer2] = useState([]);\n\n  const [selectedCard, setSelectedCard] = useState(null);\n  const [step, setStep] = useState(0);\n  const [whoseStep, setWhoseStep] = useState(0);\n\n  if (Object.keys(selectedPokemons).length === 0) {\n    history.replace('/game');\n  }\n\n  useEffect(() => {\n    const getData = async () => {\n      const boardResponse = await fetch(routes.getBoard.url);\n      const boardRequest = await boardResponse.json();\n\n      setBoard(boardRequest.data);\n\n      dispatch(asyncActions.getPlayer2Pokemons());\n    };\n\n    getData();\n  }, []);\n\n  useEffect(() => {\n    setPlayer2(() => {\n      return player2Pokemons.map((pokemon) => ({\n        ...pokemon,\n        possession: 'red',\n      }));\n    });\n  }, [player2Pokemons]);\n\n  useEffect(() => {\n    if (step === 9) {\n      const [count1, count2] = counterWin(board, player1, player2);\n\n      if (count1 === count2) {\n        dispatch(actions.addResult('draw'));\n      } else {\n        count1 > count2\n          ? dispatch(actions.addResult('win'))\n          : dispatch(actions.addResult('lose'));\n      }\n\n      setTimeout(() => {\n        history.push('/game/finish');\n      }, 5000);\n    }\n  }, [step]);\n\n  useEffect(() => {\n    const playerNumber = getFirstStepInGame(1, 2);\n\n    setTimeout(() => {\n      setWhoseStep(playerNumber);\n    }, 3000);\n  }, []);\n\n  const filterPlayerPokemons = (prevState) =>\n    prevState.filter(({ id }) => id !== selectedCard.id);\n\n  const handleClickBoardPlate = async (position) => {\n    if (selectedCard) {\n      const params = {\n        position,\n        card: selectedCard,\n        board,\n      };\n\n      const res = await fetch(routes.postCard.url, {\n        method: routes.postCard.method,\n        headers: routes.postCard.headers,\n        body: JSON.stringify(params),\n      });\n\n      const request = await res.json();\n\n      selectedCard.player === 1\n        ? setPlayer1(filterPlayerPokemons)\n        : setPlayer2(filterPlayerPokemons);\n\n      setBoard(request.data);\n\n      setStep((prevState) => {\n        const count = prevState + 1;\n        return count;\n      });\n\n      setWhoseStep((prevState) => (prevState === 1 ? 2 : 1));\n    }\n  };\n\n  const handleClickCard = (card) => {\n    setSelectedCard(card);\n  };\n\n  return (\n    <div className={s.root}>\n      <div className={s.playerOne}>\n        <PlayerBoard\n          player={1}\n          cards={player1}\n          onClickCard={(card) => handleClickCard(card)}\n          whoseStep={whoseStep === 1}\n        />\n      </div>\n\n      <div className={s.board}>\n        {board.map(({ position, card }) => (\n          <div\n            className={s.boardPlate}\n            key={position}\n            onClick={() => !card && handleClickBoardPlate(position)}\n          >\n            {card && (\n              <PokemonCard {...card} minimize={true} className={s.card} />\n            )}\n          </div>\n        ))}\n      </div>\n\n      {!selectedCard && renderArrowChoise(whoseStep)}\n\n      {result && <Result type={result} />}\n\n      <div className={s.playerTwo}>\n        <PlayerBoard\n          player={2}\n          cards={player2}\n          onClickCard={(card) => handleClickCard(card)}\n          whoseStep={whoseStep === 2}\n        />\n      </div>\n    </div>\n  );\n};\n\nexport default BoardPage;\n","/Users/buba1301/Desktop/Hexlet/ReacJS-pokemon-game/pokemon-game/src/components/Navbar/index.js",[],["178","179"],"/Users/buba1301/